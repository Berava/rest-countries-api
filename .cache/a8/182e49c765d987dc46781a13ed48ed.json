}\n\nbody {\n  scroll-behavior: smooth;\n  text-rendering: optimizeSpeed;\n}\n\n.container {\n  max-width: 1440px;\n  margin: 0 auto;\n  padding: 0 2rem;\n}\n\nimg {\n  width: 100%;\n  height: auto;\n}\n\nsection {\n  margin-top: 3.2rem;\n}\n\na {\n  text-decoration: none;\n  color: var(--clr-text);\n}\n\n/* header */\n.header {\n  background-color: var(--clr-element);\n  box-shadow: 1px 5px 7px -2px rgba(0, 0, 0, 0.05);\n}\n\n.header .container {\n  height: 6rem;\n}\n\n.header__dark-mode-toggle {\n  background-color: transparent;\n  border: none;\n  color: inherit;\n  cursor: pointer;\n  font: inherit;\n  outline: none;\n  padding: 0;\n}\n\n/* Main section */\n.filters .container {\n  padding: 0;\n}\n\n.box {\n  position: relative;\n}\n.icon {\n  color: var(--clr-input);\n  left: 15px;\n  position: absolute;\n  transform: translateY(-50%);\n  top: 50%;\n}\ninput[type='search'] {\n  border: 1px solid rgba(0, 0, 0, 0.15);\n  border-radius: 4px;\n  box-shadow: 5px 5px 15px -16px rgba(0, 0, 0, 0.55);\n  display: inline-block;\n  font-family: inherit;\n  /* height: 3.75rem; */\n  width: 25rem;\n  outline: none;\n  /* padding-left: 50px; */\n  padding: 0.8rem 0.8rem 0.8rem 50px;\n}\n\n.select-box {\n  width: 200px;\n}\n\n.select-box .options-container {\n  background-color: var(--clr-element);\n  border-radius: 4px;\n  box-shadow: 5px 5px 15px -16px rgba(0, 0, 0, 0.55);\n  color: var(--clr-text);\n  max-height: 0;\n  opacity: 0;\n  overflow: hidden;\n  transition: all 0.4s;\n  width: 100%;\n}\n\n.selected {\n  background-color: var(--clr-element);\n  border: 1px solid rgba(0, 0, 0, 0.15);\n  border-radius: 4px;\n  box-shadow: 5px 5px 15px -16px rgba(0, 0, 0, 0.55);\n  margin-bottom: 1rem;\n  position: relative;\n}\n\n.selected i {\n  transition: all 0.4s;\n}\n\n.select-box .options-container.active {\n  max-height: 240px;\n  opacity: 1;\n}\n\n.select-box .options-container.active + .selected i {\n  transform: rotateX(180deg);\n}\n\n.select-box .option,\n.selected {\n  cursor: pointer;\n  padding: 12px 24px;\n}\n\n.select-box label {\n  cursor: pointer;\n}\n\n.select-box .option:hover {\n  background-color: var(--clr-input);\n  color: var(--clr-element);\n}\n\n.select-box .option .radio {\n  display: none;\n}\n\n/* Flag section */\n.flag {\n  background-color: var(--clr-element);\n  border-radius: 5px;\n  box-shadow: 5px 5px 30px -16px rgba(0, 0, 0, 0.55);\n  cursor: pointer;\n  overflow: hidden;\n}\n\n.flag img {\n  object-fit: cover;\n  height: 160px;\n}\n\nfigcaption {\n  padding: 33px 25px 48px 25px;\n}\n\n/* country section */\n.back-button {\n  background-color: var(--clr-element);\n  box-shadow: 5px 5px 30px -16px rgba(0, 0, 0, 0.55);\n  color: var(--clr-text);\n  padding: 1em 2.5em;\n  text-decoration: none;\n}\n\n.country-text h2 {\n  margin-bottom: 3rem;\n}\n\n.country-text p {\n  margin-bottom: 0.5rem;\n}\n\n.border {\n  margin-top: 6rem;\n}\n\n.taglist {\n  list-style: none;\n}\n\n.taglist > li {\n  background-color: var(--clr-element);\n  box-shadow: 5px 5px 30px -16px rgba(0, 0, 0, 0.55);\n  color: var(--clr-text);\n  margin: 0 1rem;\n  padding: 0.25rem 0.5rem;\n  text-align: center;\n  width: 7rem;\n}\n\n/* Media Queries */\n@media (max-width: 664px) {\n  input[type='search'] {\n    width: 100%;\n  }\n}\n\n@media (max-width: 516px) {\n  .select-box {\n    margin: 2rem 0;\n  }\n\n  .flags {\n    margin-top: 0;\n  }\n\n  .country-text h2 + div div {\n    margin-bottom: 2rem;\n  }\n\n  .border {\n    margin-top: 1rem;\n  }\n\n  .taglist li {\n    margin-left: 0;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"36ab4eb6184ed553d601c472ac316d2d","cacheData":{"env":{}}}